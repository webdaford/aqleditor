/* 
 * Test case Name  : insert-into-empty-dataset-with-index.aql
 * Description     : Check that we can insert into an empty dataset and its empty secondary indexes 
 * Expected Result : Success
 * Date            : May 2 2012
 */

use dataverse test;

insert into dataset LineID (
let $x:=1
let $y:=2
let $z:=3
return {
	"l_orderkey": $x,
	"l_linenumber": $y,
	"l_suppkey": $z
}
);

insert into dataset LineID (
let $x:=2
let $y:=3
let $z:=4
return {
	"l_orderkey": $x,
	"l_linenumber": $y,
	"l_suppkey": $z
}
);

