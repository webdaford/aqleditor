drop dataverse test if exists;
create dataverse test;

use dataverse test;

create type MyRecord as closed {
  id: int32,
  point: point,
  kwds: string,
  line1: line,
  line2: line,
  poly1: polygon,
  poly2: polygon,
  rec: rectangle,
  circle: circle
}

create dataset MyData(MyRecord)
  primary key id;

create index rtree_index_point on MyData(point) type rtree;

